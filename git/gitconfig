[user]
	name = Chalise Grogan
	email = chalisegrogan@gmail.com
[push]
	default = tracking
[alias]
	gg = grep --break --heading --line-number
	ga = !"git rev-list --all | xargs git grep '$1'"
	lg = log --pretty=format:"%C(yellow bold)%h%Cred%d %Creset%s%Cgreen [%cn]" --decorate
[color]
	ui = auto
        branch = auto
        diff = auto
        status = auto
	interactive = true
[color "branch"]
        current = cyan
        local = magenta
        remote = blue
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold
[color "status"]
        added = green
        changed = magenta
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
